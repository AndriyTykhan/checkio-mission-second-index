"use strict";

function secondIndex(a, b) {
    // returns the second index of symbol in a given text

    // your code here
    return 0;
}

var assert = require('assert');

if (!global.is_checking) {
    console.log('Example')
    console.log(secondIndex("sims", "s"))
    assert.equal(secondIndex("sims", "s"), 3, "First")
    assert.equal(secondIndex("find the river", "e"), 12, "Second")
    assert.equal(secondIndex("hi", " "), undefined, "Third")
    assert.equal(secondIndex("hi mayor", " "), undefined, "Fourth")
    assert.equal(secondIndex("hi mr Mayor", " "), 5, "Fifth")
